{
  "name": "superagent-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix || true",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "generate:api": "openapi-typescript http://localhost:8080/api/v3/api-docs -o ./lib/api/generated/schema.d.ts --additional-properties --empty-objects-unknown --export-type && npx react-query-swagger /tanstack /input:http://localhost:8080/api/v3/api-docs /output:lib/api/generated/fetch-client.ts /template:Fetch && sed -i \"/^export \\* as Query from '\\.\\/fetch-client\\/Query';$/d\" lib/api/generated/fetch-client.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.5",
    "@headlessui/react": "^2.2.9",
    "@livekit/components-react": "^2.9.19",
    "@livekit/components-styles": "^1.2.0",
    "@pdfme/common": "^5.5.8",
    "@pdfme/converter": "^5.5.8",
    "@pdfme/generator": "^5.5.8",
    "@pdfme/manipulator": "^5.5.8",
    "@pdfme/schemas": "^5.5.8",
    "@pdfme/ui": "^5.5.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.26",
    "livekit-client": "^2.17.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "openapi-fetch": "^0.15.0",
    "openapi-react-query": "^0.5.1",
    "pdf-lib": "^1.17.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-toastify": "^11.0.5",
    "signature_pad": "^5.1.3",
    "simple-swizzle": "^0.2.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "validator": "^13.15.26",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/validator": "^13.15.10",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "openapi-typescript": "^7.10.1",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^3.0.0",
    "react-query-swagger": "^15.13.1",
    "tailwindcss": "^4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "pdfjs-dist": "^4.1.393"
  }
}
